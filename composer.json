{
    "name": "webpress/vn-geography",
    "description": "",
    "authors": [
        {
            "name": "duylive",
            "email": "duylive92@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [],
        "psr-4": {
            "VCComponent\\Laravel\\Geography\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Package\\Test\\": "tests/"
        }
    },
    "require": {
        "webpress/core": "^0.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^4.3",
        "spatie/phpunit-watcher": "^1.21"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "VCComponent\\Laravel\\Geography\\Providers\\GeographyServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --color=always"
        ],
        "test:watch": [
            "vendor/bin/phpunit-watcher watch"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage-html coverages"
        ]
    }
}
